{
    "name": "zoombar-vscode",
    "displayName": "zoombar",
    "description": "This Visual Studio Code Extension provides zooming UI in statusbar.",
    "version": "0.0.1",
    "publisher": "wraith13",
    "license": "SEE LICENSE IN LICENSE_1_0.txt",
    "engines": {
        "vscode": "^1.5.0"
    },
    "bugs": {
        "url": "https://github.com/wraith13/zoombar-vscode/issues"
    },
    "homepage": "https://github.com/wraith13/zoombar-vscode/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/zoombar-vscode.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "zoombar-vscode.update",
                "title": "update",
                "category": "zoombar"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
    },
    "devDependencies": {
        "typescript": "^2.9.2",
        "vscode": "^1.1.21",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}